{
  "name": "viewharx",
  "productName": "ViewHARX",
  "version": "0.0.17",
  "description": "ViewHARX is a data viewing application for HAR and SL4 formats (binary), as well as the new HARX format (zipped JSON).",
  "main": "main.js",
  "author": {
    "name": "austin.drenski",
    "email": "austin.drenski@gmail.com",
    "url": "https://github.com/austindrenski"
  },
  "homepage": "https://austindrenski.github.io/OpenGTAP",
  "readme": "https://github.com/austindrenski/OpenGTAP/blob/master/HeaderArrayConverter/ViewHARX/README.md",
  "repository": "https://github.com/austindrenski/OpenGTAP",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.6",
    "electron-builder": "^19.4.2"
  },
  "build": {
    "appId": "opengtap.viewharx",
    "win": { },
    "dmg": {
      "title": "${productName} ${version}",
      "publish": [ "github" ]
    }
  },
  "scripts": {
    "start": "electron ./app --enable-logging",
    "dist-win": "build --win --x64 --ia32",
    "dist-mac": "build --mac --x64 --ia32"
  }
}
